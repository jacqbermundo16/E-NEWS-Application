<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:autoTransition="animateToEnd"
        motion:duration="5000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/logoimageView"
               motion:framePosition="10"
               android:scaleX="1"
               android:scaleY="1" />
           <KeyAttribute
               motion:motionTarget="@+id/logoimageView"
               motion:framePosition="40"
               android:translationY="-120dp"
               android:scaleX="0.7"
               android:scaleY="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/titleTextView"
               motion:framePosition="40"
               android:alpha="0.0" />
           <KeyAttribute
               motion:motionTarget="@+id/titleTextView"
               motion:framePosition="55"
               android:alpha="1.0" />
           <KeyAttribute
               motion:motionTarget="@+id/copyrightTextView"
               motion:framePosition="65"
               android:alpha="0.0" />
           <KeyAttribute
               motion:motionTarget="@+id/copyrightTextView"
               motion:framePosition="80"
               android:alpha="1.0"
               android:translationY="-20dp" />
           <KeyAttribute
               motion:motionTarget="@+id/comlogoimageView2"
               motion:framePosition="65"
               android:alpha="0.0" />
           <KeyAttribute
               motion:motionTarget="@+id/comlogoimageView2"
               motion:framePosition="80"
               android:alpha="1.0"
               android:translationY="-20dp" />
           <KeyAttribute
               motion:motionTarget="@+id/logoimageView"
               motion:framePosition="25"
               android:translationY="-10dp" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start"></ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/copyrightTextView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="355dp"
            android:layout_height="20dp"
            android:alpha="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            android:translationY="-10dp"
            motion:layout_constraintBottom_toTopOf="@+id/comlogoimageView2" />
        <Constraint
            android:id="@+id/comlogoimageView2"
            android:layout_width="178dp"
            android:layout_height="122dp"
            android:alpha="1.0"
            android:translationY="-20dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            android:id="@+id/titleTextView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="165dp"
            android:layout_height="67dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="1.0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="117dp" />
        <Constraint
            android:id="@+id/logoimageView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="342dp"
            android:layout_height="641dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:translationY="-120dp"
            android:scaleX="0.7"
            android:scaleY="0.7" />
    </ConstraintSet>
</MotionScene>